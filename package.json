{
	"name": "nutrilyzer",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "concurrently \"vite\" \"nodemon --watch server -e ts,tsx --exec ts-node server/app.ts\"",
		"build": "vite build",
		"serve": "node dist/server/app.js",
		"lint": "eslint src server --ext .ts,.tsx",
		"lint:fix": "eslint src server --ext .ts,.tsx --fix",
		"prettier:check": "prettier --check 'src/**/*.{ts,tsx}' 'server/**/*.{ts,tsx}'",
		"prettier:write": "prettier --write 'src/**/*.{ts,tsx}' 'server/**/*.{ts,tsx}'",
		"stylelint": "stylelint 'src/**/*.css' 'src/**/*.scss'"
	},
	"dependencies": {
