		"@reduxjs/toolkit": "^2.0.1",
		"autoprefixer": "^10.4.16",
		"axios": "^1.6.3",
		"bcrypt": "^5.1.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-async-handler": "^1.2.0",
		"express-rate-limit": "^7.1.5",
		"express-validator": "^7.0.1",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"mongodb": "^6.3.0",
		"mongoose": "^8.0.3",
		"postcss": "^8.4.32",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
